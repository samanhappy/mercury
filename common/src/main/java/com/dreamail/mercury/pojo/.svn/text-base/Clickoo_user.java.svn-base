package com.dreamail.mercury.pojo;

import java.util.List;

/**
 * @author meng.sun 用户POJO类
 */
public class Clickoo_user {

	private long uid;// 用户ID

	private String name;// 用户名

	private String password;// 密码

	private int status;// 用户状态，有效状态为“0”，无效状态为“1”

	private String devicemodel; // 手机设备分辨率

	private String IMEI; // 手机IMEI

	private String signature; // 签名

	private int rolelevel; // 用户等级

	private String timedate;// 时间戳

	private long tsoffset;// 时间戳偏移量

	private int emailcount;
	
	private int sign;//是否设置黑白名单 默认0 不采用  1黑名单 2白名单
	
	private List<String> aidList;
	
	private List<Clickoo_sender_filter>  filterList; 
	
	public List<Clickoo_sender_filter> getFilterList() {
		return filterList;
	}

	public void setFilterList(List<Clickoo_sender_filter> filterList) {
		this.filterList = filterList;
	}

	public List<String> getAidList() {
		return aidList;
	}

	public void setAidList(List<String> aidList) {
		this.aidList = aidList;
	}

	public int getSign() {
		return sign;
	}

	public void setSign(int sign) {
		this.sign = sign;
	}

	public long getTsoffset() {
		return tsoffset;
	}

	public void setTsoffset(long tsoffset) {
		this.tsoffset = tsoffset;
	}

	public int getRolelevel() {
		return rolelevel;
	}

	public void setRolelevel(int rolelevel) {
		this.rolelevel = rolelevel;
	}

	public String getTimedate() {
		return timedate;
	}

	public void setTimedate(String timedate) {
		this.timedate = timedate;
	}

	public String getDevicemodel() {
		return devicemodel;
	}

	public void setDevicemodel(String devicemodel) {
		this.devicemodel = devicemodel;
	}

	public long getUid() {
		return uid;
	}

	public void setUid(long uid) {
		this.uid = uid;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIMEI() {
		return IMEI;
	}

	public void setIMEI(String iMEI) {
		IMEI = iMEI;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getSignature() {
		return signature;
	}

	public void setSignature(String signature) {
		this.signature = signature;
	}

	public int getEmailcount() {
		return emailcount;
	}

	public void setEmailcount(int emailcount) {
		this.emailcount = emailcount;
	}

}
