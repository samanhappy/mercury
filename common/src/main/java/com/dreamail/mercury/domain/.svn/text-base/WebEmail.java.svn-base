package com.dreamail.mercury.domain;


public class WebEmail {	
	private WebEmailhead head;//邮件主体
	
	private WebEmailbody body;//邮件正文
	
	private WebEmailattachment[] attach;//邮件附件
	
	private WebAccount account;
	
	private String path;

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

	public WebEmailattachment[] getAttach() {
		return attach;
	}

	public void setAttach(WebEmailattachment[] attach) {
		this.attach = attach;
	}

	public WebAccount getAccount() {
		return account;
	}

	public void setAccount(WebAccount account) {
		this.account = account;
	}

	public WebEmailhead getHead() {
		return head;
	}

	public void setHead(WebEmailhead head) {
		this.head = head;
	}

	public WebEmailbody getBody() {
		return body;
	}

	public void setBody(WebEmailbody body) {
		this.body = body;
	}




	
	
}
