	public WebAccount getAccountByReceiveServer(Clickoo_mail_account receive_account) {
		WebAccount waccount = new WebAccount();
		receive_account.loadReceiveConfig(waccount);
	
		// waccount.setUid(String.valueOf(account.getUid()));
		waccount.setName(getName());
		waccount.setPassword(new String(EmailUtils.base64TochangeByte(getPassword())));
		waccount.setAlias(getAlias());
		waccount.setComment(getComment());
		waccount.setStatus(getStatus());
		return waccount;
	}

	public WebAccount getAccountBySendServer(Clickoo_mail_account send_account) {
		WebAccount waccount = new WebAccount();
		send_account.loadSendConfig(waccount);
		
		// waccount.setUid(String.valueOf(account.getUid()));
