package com.clickoo.clickooImap.domain;

import java.io.Serializable;

public class IdleMessage implements Serializable{
	
	private static final long serialVersionUID = 1L;

	private long aid;			//账户ID
	
	private String msg;			//消息类型
	
	private String accountName;	//账户名称
	
	private int type;			//账号类型
	
	private String accountPwd;	//账户密码
	
	private String fromIp;		//发送该消息的服务器IP
	
	private int flag;			//1:pushmail 2:手机 3:pushmail+手机

	public long getAid() {
		return aid;
	}

	public void setAid(long aid) {
		this.aid = aid;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public String getAccountName() {
		return accountName;
	}

	public void setAccountName(String accountName) {
		this.accountName = accountName;
	}

	public String getAccountPwd() {
		return accountPwd;
	}

	public void setAccountPwd(String accountPwd) {
		this.accountPwd = accountPwd;
	}

	public String getFromIp() {
		return fromIp;
	}

	public void setFromIp(String fromIp) {
		this.fromIp = fromIp;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public void setFlag(int flag) {
		this.flag = flag;
	}

	public int getFlag() {
		return flag;
	}
	
}
