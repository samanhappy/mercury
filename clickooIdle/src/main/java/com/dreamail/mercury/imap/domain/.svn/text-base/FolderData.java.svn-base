package com.dreamail.mercury.imap.domain;

import java.util.List;

public class FolderData {

	/**
	 * 文件夹打开方式
	 */
	private String openWay;
	
	private List<String> delUUID;
	
	private long mailNum;
	
	public List<String> getDelUUID() {
		return delUUID;
	}

	public void setDelUUID(List<String> delUUID) {
		this.delUUID = delUUID;
	}

	public String getOpenWay() {
		return openWay;
	}

	public void setOpenWay(String openWay) {
		this.openWay = openWay;
	}

	public long getMailNum() {
		return mailNum;
	}

	public void setMailNum(long mailNum) {
		this.mailNum = mailNum;
	}
	
}
